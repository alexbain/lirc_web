{
  "name": "lirc_web",
  "description": "A NodeJS / Express app that creates a web UI + API for LIRC",
  "main": "app.js",
  "version": "0.2.4",
  "preferGlobal": true,
  "dependencies": {
    "compression": "^1.6.0",
    "consolidate": "^0.13.1",
    "express": "^4.13.0",
    "lirc_node": "0.0.4",
    "lodash": "^3.10.1",
    "morgan": "^1.6.1",
    "swig": "^1.4.2"
  },
  "devDependencies": {
    "eslint": "^1.10.0",
    "eslint-config-airbnb": "^3.1.0",
    "eslint-plugin-react": "^3.15.0",
    "grunt": "0.4.5",
    "grunt-contrib-less": "^1.1.0",
    "grunt-contrib-uglify": "^0.11.0",
    "grunt-contrib-watch": "0.6.1",
    "grunt-develop": "^0.4.0",
    "grunt-eslint": "17.3.1",
    "jquery": "^2.2.0",
    "jsdom": "^7.2.2",
    "load-grunt-tasks": "^3.4.0",
    "mocha": "2.3.4",
    "nodemon": "1.8.1",
    "should": "^8.1.1",
    "sinon": "1.17.2",
    "supertest": "1.1.0"
  },
  "bin": {
    "lirc_web": "./app.js"
  },
  "scripts": {
